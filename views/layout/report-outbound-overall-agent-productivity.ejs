<%- breadcrumb %>
  <div class="card">
    <div class="portlet-title">
      <div class="lv-header-alt clearfix">
        <h2 class="lvh-label hidden-xs">Báo cáo gọi ra - Báo tổng quát năng suất điện thoại viên</h2>
        <ul class="lv-actions actions">
          <li>
            <a data-toggle="tooltip" data-placement="top" data-original-title="Refresh">
              <i class="zmdi zmdi-refresh"></i>
            </a>
          </li>
        </ul>
      </div>
      <div class="clearfix"></div>
      <form class="form-horizontal" role="form">
        <div class="form-body">
          <div class="row m-0 m-t-20">
            <div class="col-md-4 m-b-20">
              <label for="startDate" class="control-label f-13"><span>Ngày bắt đầu</span></label>
              <div class="input-group">
                <input type="text" class="form-control pull-right search-field date-picker filter-campain input"
                  id="startDate" name="startDate" placeholder=" VD: 01/01/2016">
                <span class="input-group-addon p-l-10 bgm-gray c-white">
                  <i role="button" class="zmdi zmdi-calendar"></i>
                </span>
              </div>
            </div>
            <div class="col-md-4 m-b-20">
              <label for="endDate" class="control-label f-13"><span>Ngày kết thúc</span></label>
              <div class="input-group">
                <input type="text" class="form-control pull-right search-field date-picker filter-campain input"
                  id="endDate" name="endDate" placeholder="VD: 31/12/2016">
                <span class="input-group-addon p-l-10 bgm-gray c-white">
                  <i role="button" class="zmdi zmdi-calendar"></i>
                </span>
              </div>
            </div>

            <div class="col-md-4 m-b-20">
              <label for="idCompany" class="control-label f-13"><span>Công ty</span></label>
              <select id="idCompany" name="idCompany" class="selectpicker searchColumn input" title="Chọn công ty">
                <% for(var i=0;i < company.length;i++){ %>
                  <option value="<%- company[i]._id %>">
                    <%- company[i].name %>
                  </option>
                  <% } %>
              </select>
            </div>

          </div>
          <div class="row m-0 m-t-10">
            <div class="col-md-3 m-b-20">
            </div>
            <div class="col-md-3 m-b-20">
              <label for="idCampain" class="control-label f-13"><span>Chiến dịch</span></label>
              <select id="idCampain" name="idCampain" class="selectpicker searchColumn input" title="Chọn chiến dịch"
                multiple data-live-search="true" data-actions-box="true">
                <% for(var i=0;i < campaign.length;i++){ %>
                  <option value="<%- campaign[i]._id %>">
                    <%- campaign[i].name %>
                  </option>
                  <% } %>
              </select>
            </div>
            <div class="col-md-3 m-b-20">
              <label for="agentId" class="control-label f-13"><span>Điện thoại viên</span></label>
              <select name="agentId" class="selectpicker searchColumn input" title="Chọn điện thoại viên" multiple
                data-live-search="true" data-actions-box="true">
                <% for(var i=0;i < agent.length;i++){ %>
                  <option value="<%- agent[i]._id %>">
                    <%- agent[i].displayName %>
                  </option>
                  <% } %>
              </select>
            </div>
            <div class="col-md-3 m-b-20">
            </div>
          </div>
          <div class="row m-0 m-t-10">
            <div class="col-md-2 m-b-20">
            </div>
            <div class="col-md-4 m-b-20">
              <div class="btn-group btn-group btn-group-justified">
                <a type="button" id="searchData" class="btn bgm-blue uppercase c-white" style="width:auto">
                  <i class="fa fa-search m-r-10"></i>
                  <strong>Tìm kiếm</strong>
                </a>
              </div>
            </div>
            <div class="col-md-4 ">
              <div class="btn-group btn-group btn-group-justified">
                <a type="button" id="exportData" class="btn bgm-blue uppercase c-white" style="width:auto">
                  <i class="fa fa-search m-r-10"></i>
                  <strong>Xuất Excel</strong>
                </a>
              </div>
              <div class="col-md-2 m-b-20">
              </div>
            </div>
          </div>
        </div>
      </form>
    </div>
    <script>
    </script>
    <div class="card-body" style=" min-height:200px">
      <table class="table table-hover table-condensed table-bordered table-fix" style="table-layout: fixed;"
        id="table-agent">
        <thead>
          <tr>
            <th class="bgm-orange c-white text-center w-200"><span class="TXT_AGENT"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_TOTAL_CALL"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_TOTAL_DURATION"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_AVG_DURATION"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_TOTAL_CONNECTED"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_WAIT_DURATION"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_CALL_DURATION"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_AVG_CALL_DURATION"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_AVG_PRODUCTIVITY"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_TOTAL_TICKET"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_PROCESSED_TICKET"></span></th>
          </tr>
        </thead>
        <tbody id="body-table">
        </tbody>
        <thead id="thead-table" class="m-t-5 m-r-5">
        </thead>
      </table>
    </div>
  </div>