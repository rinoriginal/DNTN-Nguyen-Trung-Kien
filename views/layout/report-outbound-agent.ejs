<%- breadcrumb %>
<div class="card">
    <div class="portlet-title">
        <div class="lv-header-alt clearfix">
            <h2 class="lvh-label hidden-xs"><%= title %></h2>
            <ul class="lv-actions actions">
                <li><a data-toggle="tooltip" data-placement="top"
                       data-original-title="Refresh"><i
                                class="zmdi zmdi-refresh"></i></a></li>
            </ul>
        </div>
        <div class="clearfix"></div>
        <form class="form-horizontal" role="form">
            <div class="form-body">
                <div class="row m-0 m-t-20">
                    <div class="col-md-3 m-b-20">
                        <label for="startDate"
                               class="control-label f-13"><span>Ngày bắt đầu</span></label>

                        <div class="input-group">
                            <input type="text"
                                   class="form-control pull-right search-field date-picker filter-campain input"
                                   id="startDate" name="startDate"
                                   placeholder="Ngày bắt đầu, VD: 01/01/2016">
                            <span class="input-group-addon p-l-10 bgm-gray c-white"><i role="button"
                                                                                       class="zmdi zmdi-calendar"></i></span>
                        </div>
                    </div>
                    <div class="col-md-3 m-b-20">
                        <label for="endDate"
                               class="control-label f-13"><span>Ngày kết thúc</span></label>

                        <div class="input-group">
                            <input type="text"
                                   class="form-control pull-right search-field date-picker filter-campain input"
                                   id="endDate" name="endDate"
                                   placeholder="Ngày kết thúc, VD: 31/12/2016">
                            <span class="input-group-addon p-l-10 bgm-gray c-white"><i role="button"
                                                                                       class="zmdi zmdi-calendar"></i></span>
                        </div>
                    </div>
                    <div class="col-md-3 m-b-20" >
                        <label for="idCompany"
                               class="control-label f-13"><span>Công ty</span></label>
                        <select name="idCompany" class="selectpicker input" title="Chọn công ty" >
                            <option class="text-center" value="">- Chọn -</option>
                            <% for(var i = 0;i < company.length;i++){ %>
                            <option value=<%- company[i]._id %>><%-company[i].name%></option>
                            <% } %>
                        </select>
                    </div>
                    <div class="col-md-3 m-b-20" >
                        <label for="agents"
                               class="control-label f-13"><span>ĐTV</span></label>
                        <select name="agents" class="selectpicker input" title="Chọn ĐTV" multiple  data-live-search="true"
                        data-actions-box="true">
                            <% for(var i = 0;i < agents.length;i++){ %>
                            <option value=<%- agents[i]._id %> data-id=<%- agents[i].idAgentCisco %> ><%-agents[i].displayName%></option>
                            <% } %>
                        </select>
                    </div>

                </div>
                <div class="row m-0">
                    <div class="col-md-3"></div>
                    <div class="col-md-6 m-b-20">
                        <div class="btn-group btn-group btn-group-justified">
                            <a type="submit" class="btn bgm-blue uppercase c-white" style="width:auto"><i
                                        class="fa fa-search m-r-10"></i><strong>Tìm
                                    kiếm</strong></a>

                            <div class="col-md-2"></div>
                            <a id="btnExcel" class="btn bgm-blue default c-white" style="width:auto"><i
                                        class="fa fa-file-excel-o m-r-10"></i>Xuất Excel</a>
                        </div>
                    </div>
                    <div class="col-md-3"></div>
                </div>
            </div>
        </form>
    </div>



    <div class="card-body table-responsive" style=" min-height:200px">
        <table class="table table-hover table-condensed table-bordered table-responsive" style="table-layout: fixed;"
               id="exceldata">
            <thead>
            <tr>
                <th class="bgm-orange c-white text-center col-lg-2 w-120"><span class="REPORT_REASON_NAME">ĐTV</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>0h-1h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>1h-2h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>2h-3h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>3h-4h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>4h-5h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>5h-6h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>6h-7h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>7h-8h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>8h-9h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>9h-10h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>10h-11h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>11h-12h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>12h-13h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>13h-14h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>14h-15h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>15h-16h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>16h-17h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>17h-18h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>18h-19h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>19h-20h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>20h-21h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>21h-22h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>22h-23h</span></th>
                <th class="bgm-orange c-white text-center w-60"><span>23h-24h</span></th>
            </tr>
            </thead>
            <tbody id="tbBody">

            </tbody>
        </table>
        <div id="container">
        </div>
    </div>
</div>
