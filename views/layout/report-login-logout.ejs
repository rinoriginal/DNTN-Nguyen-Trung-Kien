<%- breadcrumb %>
  <script>
  </script>
  <div class="card" style="margin-bottom: 100px;">
    <div class="portlet-title">
      <div class="lv-header-alt clearfix">
        <h2 class="lvh-label hidden-xs">
          <%= title %>
        </h2>
        <ul class="lv-actions actions">
          <li>
            <a data-toggle="tooltip" data-placement="top" data-original-title="Refresh">
              <i class="zmdi zmdi-refresh"></i>
            </a>
          </li>
        </ul>
      </div>
      <div class="clearfix"></div>
      <form class="form-horizontal" role="form" id="form">
        <div class="form-body">
          <div class="row m-0 m-t-20">
            <input type="hidden" value="1" id="isFilter" name="isFilter">
            <div class="col-md-2 m-b-20"></div>
            <div class="col-md-4 m-b-20">
              <label for="startDate" class="control-label f-13"><span>Thời gian</span></label>
              <div class="input-group">
                <input type="text" class="form-control pull-right search-field multi-date-picker input" name="dateTime"
                  placeholder="VD: 01/01/2016" autocomplete="off">
                <span class="input-group-addon p-l-10 bgm-gray c-white">
                  <i role="button" class="zmdi zmdi-calendar"></i>
                </span>
              </div>
            </div>
            <div class="col-md-4 m-b-20">
              <label for="companies" class="control-label f-13"><span>Điện thoại viên</span></label>
              <select name="agents" class="selectpicker input" title="Chọn điện thoại viên" multiple
                data-actions-box="true" data-select-all-text="Chọn tất cả" data-deselect-all-text="Bỏ chọn tất cả">
                <% for(var i=0; i < agent.length; i++){ %>
                  <option value="<%- agent[i]._id %>">
                    <%- agent[i].displayName %>
                  </option>
                  <% } %>
              </select>
            </div>
            <div class="col-md-2 m-b-20"></div>
          </div>
          <div class="row m-0">
            <div class="col-md-3"></div>
            <div class="col-md-6 m-b-20">
              <div class="btn-group btn-group btn-group-justified" style="table-layout: auto !important;">
                <div class="col-md-4"></div>
                <a id="search_data" type="button" class="btn bgm-blue w-100 c-white">
                  <i class="fa fa-search m-r-10"></i>
                  <strong>Tìm kiếm</strong>
                </a>
                <div class="col-md-1"></div>
                <a id="export_excel_total" class="btn bgm-blue w-100 c-white">
                  <i class="fa fa-file-excel-o m-r-10"></i>
                  Xuất Excel
                </a>
                <div class="col-md-4"></div>
              </div>
            </div>
            <div class="col-md-3"></div>
          </div>
        </div>
      </form>
    </div>

    <div class="card-body table-responsive">
      <div class="lv-header-alt clearfix">
        <h2 class="lvh-label hidden-xs">Tổng</h2>
      </div>
      <table class="table table-hover table-condensed table-bordered table-fix" style="table-layout: fixed;"
        id="exceldata">
        <thead>
          <tr>
            <th class="bgm-orange c-white text-center"><span class="TXT_NAME"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_FIRST_LOGIN_TIME"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_LAST_LOGOUT_TIME"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_TOTAL_ONLINE_DUR"></span></th>
            <th class="bgm-orange c-white text-center"><span class="TXT_AVG_ONLINE_DUR"></span></th>
          </tr>
        </thead>
        <tbody id="table_total"></tbody>
      </table>
    </div>

    <hr />

    <div id="detail-by-day">
      <div class="card-body table-responsive">
        <div class="lv-header-alt clearfix">
          <h2 class="lvh-label hidden-xs">Chi tiết theo ngày</h2>
        </div>
        <table class="table table-hover table-condensed table-bordered table-fix" style="table-layout: fixed;"
          id="exceldatagroupbyday">
          <thead>
            <tr>
              <th class="bgm-orange c-white text-center"><span class="TXT_NAME"></span></th>
              <th class="bgm-orange c-white text-center"><span class="TXT_FIRST_LOGIN_TIME"></span></th>
              <th class="bgm-orange c-white text-center"><span class="TXT_LAST_LOGOUT_TIME"></span></th>
              <th class="bgm-orange c-white text-center"><span class="TXT_TOTAL_ONLINE_DUR"></span></th>
              <th class="bgm-orange c-white text-center"><span class="TXT_AVG_ONLINE_DUR"></span></th>
            </tr>
          </thead>
          <tbody id="table_by_day"></tbody>
        </table>
      </div>
    </div>

    <hr />

    <div class="card-body table-responsive">
      <div class="lv-header-alt clearfix">
        <h2 class="lvh-label hidden-xs">Chi tiết thời gian tổng quát</h2>
      </div>
      <div id="timelines" class="chart-container" style="overflow-x: hidden; overflow-y: hidden; min-height: 300px;">
      </div>
    </div>

    <div class="card-body table-responsive" id="time_by_day">

    </div>

  </div>