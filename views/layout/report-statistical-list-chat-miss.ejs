<%-breadcrumb%>
<div class="card">
    <div class="portlet-title">
        <div class="lv-header-alt clearfix m-l-10">
            <h2 class="lvh-label hidden-xs">
                <%= title %>
            </h2>
            <ul class="lv-actions actions">
                <!-- <li>
                    <a href="/#ticket-advisory/new" data-toggle="tooltip" data-placement="top" id="addTernal"
                        data-original-title="Tạo mới">
                        <i class="zmdi zmdi-plus"></i>
                    </a>
                </li> -->
                <li>
                    <a data-toggle="tooltip" data-placement="top" data-original-title="Refresh" id="reload"><i
                            class="zmdi zmdi-refresh"></i></a>
                </li>
            </ul>
        </div>
        <div class="clearfix"></div>


        <div class="panel panel-default m-l-30 m-r-30 box-shadow">
            <div class="panel-heading p-0">
                <h5 class="panel-title p-l-10">
                    <div data-id="showCustomer" class="btn-block p-10 bgm-orange c-white text-uppercase">Tra cứu</div>
                </h5>
            </div>
            <div id="collapseOne" class="panel-collapse collapse in ticketCustomerCollapse">
                <div id="form-filter">
                    <div class="form-body">
                        <div class="row m-l-0 m-r-0 m-t-20 m-b-0 p-10" style="display: flex;">
                            <div class="col-md-2 ">
                                <label for="created" class="control-label f-13"><span>Khoảng thời gian</span></label>
                                <div class="input-group">
                                    <input type="text"
                                        class="form-control pull-right search-field multi-date-picker filter-campain input"
                                        id="created" name="created" placeholder=" VD: 01/01/2020 - 01/01/2021">
                                    <span class="input-group-addon p-l-10 bgm-gray c-white"><i role="button"
                                            class="zmdi zmdi-calendar"></i></span>
                                </div>
                            </div>

                            <div class="col-md-2 m-b-20">
                                <label for="type" class="control-label f-13"><span>Số điện thoại KH</span></label>
                                <input type="text" class="form-control search-field filter-campain input"
                                    id="phoneNumber" name="phoneNumber" placeholder="">
                            </div>
                            <div class="col-md-2 m-b-20">
                                <label for="type" class="control-label f-13"><span>Họ tên KH</span></label>
                                <input type="text" class="form-control search-field filter-campain input"
                                    id="nameCustomer" name="nameCustomer" placeholder="">
                            </div>
                            <div class="col-md-2 m-b-20">
                                <label for="type" class="control-label f-13"><span>Email </span></label>
                                <input type="text" class="form-control search-field filter-campain input" id="email"
                                    name="email" placeholder="">
                            </div>
                            <div class="col-md-2 m-b-20">
                                <label for="channel" class="control-label f-13"><span>Kênh chat</span></label>
                                <select name="channel" class="selectpicker input" data-live-search="true"
                                    data-actions-box="true" title="Chọn kênh chat" multiple>
                                    <% channel.forEach(function(item){%>
                                    <option value="<%- item._id%>"><%- item.name%></option>
                                    <%})%>

                                </select>
                            </div>
                            <div class="col-md-2 m-b-20">
                                <label for="agent" class="control-label f-13"><span>Agent</span></label>
                                <select name="agent" class="selectpicker input" data-live-search="true"
                                    data-actions-box="true" title="Chọn Agent" multiple>
                                    <% Agent.forEach(function(item){%>
                                    <option value="<%- item._id%>"><%- item.displayName%></option>
                                    <%})%>

                                </select>
                            </div>

                        </div>
                        <div class="row m-0 m-b-10">
                            <div class="col-md-5"></div>
                            <div class="col-md-2 m-b-20 text-center">
                                <button
                                    class="btn f-12 waves-effect text-uppercase  bgm-btn-search c-white border-radius"
                                    id="searchTernal">
                                    Tra cứu</button>
                            </div>
                            <div class="col-md-5"></div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
    <div class="row m-t-20 m-b-10 m-r-30 text-right">
        <button class="btn btn-warning f-12 btn-save waves-effect text-uppercase  border-radius m-r-15"
            id="exportexcel">
            export</button>
    </div>
    <div class="div_table m-l-30 m-r-30 box-shadow">
        <table class="table table-hover table-condensed table-bordered table-fix" id="exceldata">
            <thead id="thead-table" class="m-t-5 m-r-5">
                <tr>
                    <th class="bgm-orange c-white text-center bgm-confix-header" style="width: 50px;"><span
                            class="TXT_STT"></span>
                    </th>
                    <th class="bgm-orange c-white text-center bgm-confix-header"><span class="TXT_TIME_START"></span>
                    </th>
                    <th class="bgm-orange c-white text-center bgm-confix-header"><span class="TXT_NAME_CUSTOMER"></span>
                    </th>
                    <th class="bgm-orange c-white text-center bgm-confix-header"><span class="TXT_PHONE"></span></th>
                    <th class="bgm-orange c-white text-center bgm-confix-header"><span class="TXT_EMAIL"></span></th>
                    <th class="bgm-orange c-white text-center bgm-confix-header"><span class="TXT_AGENT"></span>
                    </th>
                    <th class="bgm-orange c-white text-center bgm-confix-header"><span class="TXT_TIME_WAIT"></span>
                    </th>
                    <th class="bgm-orange c-white text-center bgm-confix-header"><span class="TXT_CHANNEL_CHAT"></span>
                    </th>
                    <th class="bgm-orange c-white text-center bgm-confix-header"><span class="TXT_ACTION"></span></th>
                </tr>
            </thead>

            <tbody id="tbBody">

            </tbody>
        </table>


        <div id="container">
        </div>
        <div class="text-center" id="paging"></div>
    </div>

