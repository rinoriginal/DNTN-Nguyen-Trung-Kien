<%- breadcrumb %>
<script>
    var _CallTypes = `<%- encodeURIComponent(JSON.stringify(callTypes)); %>`;
</script>

<div class="card">
    <div class="portlet-title">
        <div class="lv-header-alt clearfix">
            <h2 class="lvh-label hidden-xs">
                <%= title %>
            </h2>
            <ul class="lv-actions actions">
                <li><a data-toggle="tooltip" data-placement="top" data-original-title="Refresh"><i
                                class="zmdi zmdi-refresh"></i></a></li>
            </ul>
        </div>
        <div class="clearfix"></div>
        <form class="form-horizontal" role="form">
            <div class="form-body">
                <div class="row m-0 m-t-20">
                    <div class="col-md-4 m-b-20">
                        <label for="startDate" class="control-label f-13"><span>Ngày bắt đầu</span></label>

                        <div class="input-group">
                            <input type="text" class="form-control pull-right search-field date-picker filter-campain input" id="startDate" name="startDate"
                                   placeholder="Ngày bắt đầu, VD: 01/01/2016">
                            <span class="input-group-addon p-l-10 bgm-gray c-white"><i role="button"
                                                                                       class="zmdi zmdi-calendar"></i></span>
                        </div>
                    </div>
                    <div class="col-md-4 m-b-20">
                        <label for="endDate" class="control-label f-13"><span>Ngày kết thúc</span></label>

                        <div class="input-group">
                            <input type="text" class="form-control pull-right search-field date-picker filter-campain input" id="endDate" name="endDate"
                                   placeholder="Ngày kết thúc, VD: 31/12/2016">
                            <span class="input-group-addon p-l-10 bgm-gray c-white"><i role="button"
                                                                                       class="zmdi zmdi-calendar"></i></span>
                        </div>
                    </div>

                    <div class="col-md-4 m-b-20">
                        <label for="skillGroups" class="control-label f-13"><span>Nhóm</span></label>
                        <select name="skillGroups" id="skillGroups" class="selectpicker input" title="Chọn nhóm" multiple>
                            <% for(var i = 0;i < skillGroups.length;i++){ %>
                            <option value=<%- skillGroups[i].SkillTargetID %>><%- skillGroups[i].EnterpriseName %></option>
                            <% } %>
                        </select>
                    </div>

                </div>
                <div class="row m-0">
                    <div class="col-md-3 col-md-offset-3 m-b-20">
                        <div class="btn-group btn-group btn-group-justified">
                            <a type="submit" id="report-overall" class="btn bgm-blue uppercase c-white" style="width:auto">
                                <i class="fa fa-search m-r-10"></i>
                                <strong>Tìm kiếm</strong>
                            </a>
                        </div>
                    </div>
                    <div class="col-md-3 m-b-20">
                        <div class="btn-group btn-group btn-group-justified">
                            <a id="exportexcel-overall" class="btn bgm-blue default c-white" style="width:auto">
                                <i class="fa fa-file-excel-o m-r-10"></i> Xuất Excel</a>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>



    <div class="card-body table-responsive" style=" min-height:200px">
        <table class="table table-hover table-condensed table-bordered table-fix" style="table-layout: fixed;" id="exceldata-overall">
            <thead>
            <tr>
                <th class="bgm-orange c-white text-center"><span class="TXT_INDEX_MONITOR"></span></th>
                <th class="bgm-orange c-white text-center"><span class="TXT_SKILL_GROUP"></span></th>
                <th class="bgm-orange c-white text-center"><span class="TXT_TARGET"></span></th>
                <th class="bgm-orange c-white text-center"><span class="TXT_RESULT"></span></th>
                <th class="bgm-orange c-white text-center"><span class="TXT_RATIO"></span></th>
            </tr>
            </thead>
            <tbody id="tbBody-overall">

            </tbody>
        </table>
        <div id="container">
        </div>
    </div>

</div>